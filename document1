const { test, expect } = require('@playwright/test');

test('Check API endpoint for 200 response', async ({ page }) => {
    // Set custom header
    const headers = {
        'Content-Type': 'application/xml'
    };

    // URL to test
    const url = 'https://ws-content-cds-fac-fmr.com/cds/v1/2641/381752?requestkey=hsadkjaq312jmc931jdsldjdka23';

    // Perform the request
    const response = await page.goto(url, {
        method: 'GET',
        headers: headers,
        waitUntil: 'domcontentloaded'
    });

    // Check if response status is 200
    expect(response).not.toBeNull();
    expect(response.status()).toBe(200);
    
    // Log response body
    const responseBody = await response.text();
    console.log('Response status is 200. Response body:', responseBody);
});
