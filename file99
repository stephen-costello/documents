const { test, expect } = require('@playwright/test');

test('Test 1 - Visit URL 1', async ({ page }) => {
  await page.goto('https://your-url-1.com');
  // Add your test logic for the first URL
  expect(await page.title()).toBe('Expected Title for URL 1');
});

