const { test, expect } = require('@playwright/test');

test('Click on My Activities element', async ({ page }) => {
  // Navigate to the specified URL
  await page.goto('http://hahahah.com');

  // Wait for the element to be present in the DOM
  const myActivitiesElement = await page.waitForSelector('s-assigned-wrapper');

  // Click on the My Activities element
  await myActivitiesElement.click();

  // Wait for some time to see the result (you can adjust this as needed)
  await page.waitForTimeout(2000);

  // Add an assertion to check if the click was successful or any other conditions
  const pageTitle = await page.title();
  expect(pageTitle).toBe('Expected Page Title'); // Replace 'Expected Page Title' with the actual expected title
});
