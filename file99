  await page.goto('https://your-url-2.com');
  // Add your test logic for the second URL
  expect(await page.title()).toBe('Expected Title for URL 2');
});

test('Test 3 - Visit URL 3', async ({ page }) => {
  await page.goto('https://your-url-3.com');
  // Add your test logic for the third URL
  expect(await page.title()).toBe('Expected Title for URL 3');
