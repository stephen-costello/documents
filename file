const { http } = require('playwright');

(async () => {
  try {
    // Send a GET request to the given endpoint
    const response = await http.get('http://ws-conten-cds-fac.fmr.com');

    // Check if the response status is 200
    if (response.status() === 200) {
      console.log('FAC is alive');
    } else {
      console.error('FAC is not responding with a 200 status code');
    }
  } catch (error) {
    console.error('Error occurred while accessing the endpoint:', error.message);
  }
})();
